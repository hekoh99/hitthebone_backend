// This is your Prisma schema file

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Movie {
  id        Int     @id @default(autoincrement()) @unique
  title     String
  year      Int
  createdAt  DateTime   @default(now())
}
